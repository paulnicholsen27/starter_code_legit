<h1> New Owner </h1>

<% if @owner.errors.any? %>
<% @owner.errors.full_messages.each do |message| %>
  <%= message %>
  <br>
  <% end %>
<% end %>
<%= form_for @owner do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.collection_check_boxes :pet_ids, Pet.all, :id, :name %>

  <%= f.submit %>
<% end %>
